  <ItemGroup>
    <ProjectReference Include="..\GymMenagmentApp.Shared\GymMenagmentApp.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="ConsoleApp1">
      <HintPath>ConsoleApp1.dll</HintPath>
    </Reference>
    <Reference Include="DataBase.API">
      <HintPath>DataBase.API.dll</HintPath>
    </Reference>
    <Reference Include="DataBaseAPI">
      <HintPath>DataBaseAPI.dll</HintPath>
